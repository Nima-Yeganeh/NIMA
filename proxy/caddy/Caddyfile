zuaecdnservice1.zizigooloovpn.com {

  root * /usr/share/caddy

  @websockets {
    header Connection *Upgrade*
    header Upgrade    websocket
  }

  reverse_proxy @websockets 20.46.155.15:8443/ws

  route {
    reverse_proxy /ws 20.46.155.15:8443
    file_server
  }

  log {
    output stdout
  }

}

